.post {
    padding: 1.5em 2em;
    border: 1px #ddd;
    border-style: solid none;
    h1.title {
        margin: 0;
        display: flex;
        justify-content: center;
    }
    h2 {
        font-size: 1.2em;
    }
    h3 {
        font-size: 1.1em;
    }
    h4, h5, h6{
        font-size:1.0em
    }
    a {
        color: gray;
        text-decoration: underline;
    }
    ul {
        padding-left: 30px;
    }
    ol {
        padding-left: 20px;
    }
    blockquote {
        border-left: 2px solid #616161;
        padding-left: 20px;
        margin: 1rem 0;
    }
    p code {
        padding: 0.1em;
        border: 2px solid #ccc;
        border-radius: 4px;
    }
    li code{
        padding: 0.1em;
        border: 2px solid #ccc;
        border-radius: 4px;
    }
    code, pre {
        font-size: 16px;
        background-color: #E5E2E2;
        font-family: 'Fantasque Sans Mono', 'PT Mono', 'Source Code Pro', monospace;
        
    }
    .highlight {
        position: relative;
        margin: 1em 0;
        border-radius: 2px;
        line-height: 1.1em;
        background-color: #f8f8f8;
        overflow-x: auto;
    }
    .highlight table, .highlight tr, .highlight td {
        width: 100%;
        border-collapse: collapse;
        padding: 0;
        margin: 0
    }
    .highlight .gutter {
        /*padding-left: 10px;
    width: 10px;*/
        display: none;
    }
    .highlight .code pre {
        line-height: 1em;
        margin: 0;
    }
    .highlight .code pre .line {
        width: auto;
        height: 18px;
    }
    pre {
        border: 1px solid #ccc;
        border-radius: 4px;
        padding: 0.3em;
        color: #525252;
    }
}

@media screen and (max-width: 320px) {
    .post {
        padding: 30px 16px;
    }
}

.prev_next {
    padding: 1em;
    margin: 1em;
    border-style: solid none none none;
    border: 1px #ddd;
    .next {
        text-align: right;
        text-decoration: none;
        color: gray;
    }
    .prev {
        text-decoration: none;
        color: gray;
    }
    display: flex;
    justify-content: space-between;
}

.posts {
    .post-title {
        font-size: 1.5em;
        justify-content: center;
        display: flex;
        .post-title-link {
            color: #000;
            font-weight: normal;
            text-decoration: none;
        }
    }
    .post-content {
        margin-bottom: 1em;
        a {
            color: gray;
            text-decoration: underline;
        }
        .More {
            padding: 1em 0;
        }
    }
    .post-meta {
        color: black;
    }
}
